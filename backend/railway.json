{
    "build": {
      "builder": "NIXPACKS",
      "nixpacksConfig": {
        "phases": {
          "setup": {
            "nixPkgs": ["nodejs"]
          },
          "install": {
            "cmds": ["npm install"]
          },
          "build": {
            "cmds": ["npm run build"]
          }
        }
      }
    }
  }
  